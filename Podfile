# Uncomment the next line to define a global platform for your project
 platform :ios, '10.0'

def shared_pods
    
    pod 'Firebase/Storage'
    
    pod 'Firebase/Auth'
    pod 'Firebase/Database'
    pod 'Firebase/Core'

    pod 'JVFloatLabeledTextField'
    pod 'SwiftyJSON'
    pod 'SDWebImage', '~>4.0'
    pod 'IQKeyboardManagerSwift'
    pod 'FBSDKLoginKit'
    pod 'FacebookLogin'
    pod 'FacebookCore'
    pod 'ZFTokenField'
    pod 'SimpleImageViewer', '~> 1.1.1'
    pod "GSMessages"
end


target 'Peps' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Peps
    shared_pods
end


target 'PepsAdmin' do
use_frameworks!
    shared_pods
end

